datasource db {
  provider = "postgresql"
}



generator client {
  provider = "prisma-client-js"
}


model Blog {
  id        Int      @default(autoincrement()) @id
  title     String
  content   String?
  admin  String
  password  string
  published Boolean? @default(false)

}

model user {
    id        Int      @default(autoincrement()) @id
  username String
  password String
}

